#!/Users/py356p/codebase/pact-python/venv27/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pact-python','console_scripts','pact-verifier'
__requires__ = 'pact-python'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pact-python', 'console_scripts', 'pact-verifier')()
    )
